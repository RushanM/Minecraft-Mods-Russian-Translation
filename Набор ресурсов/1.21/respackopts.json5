{
  id: "modsru",
  version: 13,
  capabilities: ["FileFilter", "DirFilter"],
  conf: {
    general: {
      enableCustomTranslations: true,
      fixAftercolonWordsCase: false,
      modsCreativeTabsStyle: ["modItems", "items", "original"],
      modsWordStyle: ["mod", "modification", "hybrid"],
      removeEllipsis: false,
      removeModInTatar: true,
      translateJokes: true,
      translateModNames: false,
      translateModNamesOnAdvancements: false,
      yourModloader: ["any", "neoforgeOrForge", "fabricOrQuilt"]
    },
    specific: {
      cobblemon: {
        apricornInsteadOfAbrizholud: {
          type: "boolean",
          default: false
        },
        naturaInsteadOfKharakter: {
          type: "boolean",
          default: false
        },
        pokemonNamesTranslationStyle: {
          type: "enum",
          default: "translit",
          values: ["translit", "sienduk"]
        }
      },
      enigmaticLegacyColoredTabs: {
        type: "boolean",
        default: true
      },
      materialEnergyInsteadOfMatterEnergy: {
        type: "boolean",
        default: false
      },
      modMenuEasterEggsFormalStyle: {
        type: "boolean",
        default: true
      },
      shortResetAllForControlling: {
        type: "boolean",
        default: true
      }
    }
  }
}