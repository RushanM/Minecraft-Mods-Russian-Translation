{
  "id": "modsru",
  "version": 13,
  "capabilities": ["FileFilter"],
  "conf": {
    "general": {
      "fixAftercolonWordsCase": {
        "type": "boolean",
        "default": false,
        "reloadType": "Resource"
      },
      "modsCreativeTabsStyle": {
        "type": "enum",
        "default": "modItems",
        "values": ["modItems", "items", "original"],
        "reloadType": "Resource"
      },
      "modsWordStyle": {
        "type": "enum",
        "default": "mod",
        "values": ["mod", "modification", "hybrid"],
        "reloadType": "Resource"
      },
      "removeEllipsis": {
        "type": "boolean",
        "default": false,
        "reloadType": "Resource"
      },
      "removeModInTatar": {
        "type": "boolean",
        "default": true,
        "reloadType": "Resource"
      },
      "translateJokes": {
        "type": "boolean",
        "default": true,
        "reloadType": "Resource"
      },
      "translateModNames": {
        "type": "boolean",
        "default": false,
        "reloadType": "Resource"
      },
      "translateModNamesOnAdvancements": {
        "type": "boolean",
        "default": false,
        "reloadType": "Resource"
      },
      "yourModloader": {
        "type": "enum",
        "default": "any",
        "values": ["any", "neoforgeOrForge", "fabricOrQuilt"],
        "reloadType": "Resource"
      }
    },
    "specific": {
      "cobblemon": {
        "apricornInsteadOfAbrizholud": {
          "type": "boolean",
          "default": false,
          "reloadType": "Resource"
        },
        "naturaInsteadOfKharakter": {
          "type": "boolean",
          "default": false,
          "reloadType": "Resource"
        },
        "pokemonNamesTranslationStyle": {
          "type": "enum",
          "default": "translit",
          "values": ["translit", "sienduk"],
          "reloadType": "Resource"
        }
      },
      "enigmaticLegacyColoredTabs": {
        "type": "boolean",
        "default": true,
        "reloadType": "Resource"
      },
      "materialEnergyInsteadOfMatterEnergy": {
        "type": "boolean",
        "default": false,
        "reloadType": "Resource"
      },
      "modMenuEasterEggsFormalStyle": {
        "type": "boolean",
        "default": true,
        "reloadType": "Resource"
      },
      "shortResetAllForControlling": {
        "type": "boolean",
        "default": true,
        "reloadType": "Resource"
      }
    }
  }
}