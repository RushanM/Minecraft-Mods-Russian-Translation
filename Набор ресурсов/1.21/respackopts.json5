{
  id: "modsru",
  version: 13,
  capabilities: ["FileFilter"],
  conf: {
    general: {
      fixAftercolonWordsCase: {
        type: "boolean",
        default: false,
      },
      modsCreativeTabsStyle: {
        type: "enum",
        default: "modItems",
        values: ["modItems", "items", "original"]
      },
      modsWordStyle: {
        type: "enum",
        default: "mod",
        values: ["mod", "modification", "hybrid"]
      },
      removeEllipsis: {
        type: "boolean",
        default: false
      },
      removeModInTatar: {
        type: "boolean",
        default: true
      },
      translateJokes: {
        type: "boolean",
        default: true
      },
      translateModNames: {
        type: "boolean",
        default: false
      },
      translateModNamesOnAdvancements: {
        type: "boolean",
        default: false
      },
      yourModloader: {
        type: "enum",
        default: "any",
        values: ["any", "neoforgeOrForge", "fabricOrQuilt"]
      }
    },
    specific: {
      cobblemon: {
        apricornInsteadOfAbrizholud: {
          type: "boolean",
          default: false
        },
        naturaInsteadOfKharakter: {
          type: "boolean",
          default: false
        },
        pokemonNamesTranslationStyle: {
          type: "enum",
          default: "translit",
          values: ["translit", "sienduk"]
        }
      },
      enigmaticLegacyColoredTabs: {
        type: "boolean",
        default: true
      },
      materialEnergyInsteadOfMatterEnergy: {
        type: "boolean",
        default: false
      },
      modMenuEasterEggsFormalStyle: {
        type: "boolean",
        default: true
      },
      shortResetAllForControlling: {
        type: "boolean",
        default: true,
      enableCustomTranslations: {
        type: "boolean",
        default: true // Default to true to enable custom translations when Respackopts is installed
    }
  }
}